    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "18"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "express": "^4.21.2",
    "firebase-admin": "^12.0.0",
    "firebase-functions": "^5.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "typescript": "^5.6.3"
  }
}
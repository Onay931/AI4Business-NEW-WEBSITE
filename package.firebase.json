{
  "name": "ai4business-functions",
  "version": "1.0.0",
  "description": "Firebase Functions for AI4Business website",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "serve": "npm run build && firebase emulators:start --only functions",
